{
  "name": "enhanced-working-diffs",
  "displayName": "enhanced working diffs",
  "description": "",
  "version": "0.0.4",
  "publisher": "gosvig123",
  "engines": {
    "vscode": "1.96.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "enhanced-working-diffs.toggleInlineDiff",
        "title": "Toggle Inline Working Diff",
        "category": "Diff"
      }
    ],
    "keybindings": [
      {
        "command": "enhanced-working-diffs.toggleInlineDiff",
        "key": "ctrl+alt+m",
        "mac": "cmd+alt+m",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "prepublish": "webpack --config webpack.config.js",
    "webpack": "webpack --config webpack.config.js",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "vscode-test",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.2",
    "@types/webpack": "^5.28.5",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^2.15.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "simple-git": "^3.27.0"
  }
}
